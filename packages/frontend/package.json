{
  "name": "nitrix-frontend",
  "version": "1.0.0",
  "description": "Nitrix AI Training Platform - Frontend Application",
  "main": "dist/index.js",
  "type": "module",
  "author": "Nitrix Team",
  "license": "MIT",
  "homepage": "https://nitrix.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/nitrix.git",
    "directory": "packages/frontend"
  },
  "bugs": {
    "url": "https://github.com/your-org/nitrix/issues"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "no-code",
    "privacy-first",
    "local-training"
  ],
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "build:production": "NODE_ENV=production vite build",
    "postbuild": "cp dist/index.html dist/404.html",
    "serve": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:perf": "lhci collect --url=http://localhost:5173",
    "report:perf": "lhci report",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "vite-bundle-analyzer dist/stats.html"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.0.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "chart.js": "^4.4.1",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.2",
    "date-fns": "^3.0.6",
    "dexie": "^3.2.4",
    "framer-motion": "^10.18.0",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.21.0",
    "react-use": "^17.4.0",
    "recharts": "^2.9.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.8",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.2",
    "vite": "^5.1.4",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(.*\\.mjs$|@testing-library))"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.tsx",
      "!src/vite-env.d.ts",
      "!src/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
