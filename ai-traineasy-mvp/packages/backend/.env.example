# AI TrainEasy MVP Configuration
# Copy this to .env and update the values

# Security
SECRET_KEY=your-super-secret-key-change-this-in-production-make-it-long-and-random
ENVIRONMENT=development

# Database
DATABASE_URL=sqlite:///./ai_traineasy.db

# External APIs
HUGGINGFACE_HUB_TOKEN=your-huggingface-token-here

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=3600

# Training Limits
MAX_TRAINING_TIME_SECONDS=3600
MAX_FILE_SIZE_MB=100
MAX_PROJECTS_PER_USER=10